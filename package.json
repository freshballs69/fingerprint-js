{
  "name": "fingerprint-js",
  "version": "1.3.0",
  "type": "module",
  "description": "Browser fingerprinting library",
  "main": "dist/fingerprint.cjs.js",
  "module": "dist/fingerprint.esm.js",
  "browser": "dist/fingerprint.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fingerprint.esm.js",
      "require": "./dist/fingerprint.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freshballs69/fingerprint-js.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "fingerprint",
    "browser",
    "tracking",
    "identification"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.56.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
